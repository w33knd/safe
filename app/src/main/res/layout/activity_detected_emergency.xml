<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.DetectedEmergencyActivity">

    <ImageButton
        android:id="@+id/mainEmergencyButton2"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="#00000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/emergency_circle_black" />

    <ImageView
        android:id="@+id/sosEmergencyImage2"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/connectEmegencyImage2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/emergency_circle" />

    <ImageView
        android:id="@+id/connectEmegencyImage2"
        android:layout_width="75dp"
        android:layout_height="75dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sosEmergencyImage2"
        app:srcCompat="@drawable/emergency_circle" />

    <ImageView
        android:id="@+id/findingEmergencyImage2"
        android:layout_width="75dp"
        android:layout_height="75dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/connectEmegencyImage2"
        app:layout_constraintTop_toTopOf="@+id/sosEmergencyImage2"
        app:srcCompat="@drawable/emergency_circle" />
</androidx.constraintlayout.widget.ConstraintLayout>